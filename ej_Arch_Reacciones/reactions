FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "constant";
  object      reactions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
	
reactionModel secondOrderv2;

reactions
{
    ABReaction
    {
        reaction  "A^1 + B^1 = C^0";
        kf  kf [0 3 -1 0 -1 0 0] 0.001;
    }
	
	CDReaction
    {
        reaction  "C^1 = D^1";
        kf  kf [0 3 -1 0 -1 0 0] 0.001;
		liesegang liesegang [0 0 0 0 0 0 0] 2;
		rho rho [0 -3 0 0 1 0 0] 9e15;
    }
	
	CReaction
    {
        reaction  "C^1 = D^0";
		kf  kf [0 0 -1 0 0 0 0] 0.001;
		cs  cs [0 -3 0 0 1 0 0] 6;
		liesegang liesegang [0 0 0 0 0 0 0] 1;
    }
}

